        "\n",
        "from psmiles import PolymerSmiles as PS\n",
        "\n",
        "# PSMILES string of polystyrene\n",
        "polystyrene = \"C(c1ccccc1)(C[*])[*]\"\n",
        "ps = PS(polystyrene)\n",
        "ps"
